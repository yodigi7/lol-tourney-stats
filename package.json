{
  "name": "server",
  "version": "1.0.0",
  "description": "Backend server",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && prettier --check src/**/*.ts && npm run mocha",
    "build": "tsc",
    "fmt": "prettier --write src/**/*.ts test/**/*.ts",
    "start": "ts-node-dev --respawn --clear ./src/index.ts",
    "debug": "ts-node-dev --respawn --clear ./src/index.ts",
    "mocha": "mocha -r ts-node/register test/**/*.spec.ts",
    "lint": "eslint src/**/*.ts --ext .js,.ts",
    "dev": "node ./dist/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yodigi7/lol-tourney-stats.git"
  },
  "keywords": [
    "riot",
    "league of legends",
    "LoL",
    "games",
    "Riot"
  ],
  "author": "yodigi7",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yodigi7/lol-tourney-stats/issues"
  },
  "homepage": "https://github.com/yodigi7/lol-tourney-stats#readme",
  "dependencies": {
    "express": "^4.17.1",
    "node-env-file": "^0.1.8",
    "node-fetch": "^2.6.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.20",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "chai": "^4.2.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "mocha": "^6.2.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.3"
  }
}
